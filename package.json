    "express-validator": "3.1.2",
    "jsonwebtoken": "7.2.1",
    "pg": "6.1.2",
    "redis": "2.6.5",
    "socket.io": "1.7.2",
    "uws": "0.12.0"
  },
  "devDependencies": {
    "morgan": "^1.7.0"
  }
}
